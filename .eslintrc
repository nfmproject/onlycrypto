{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false,
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "prefer-promise-reject-errors": ["off"],
      "react/jsx-filename-extension": ["off"],
      "react/prop-types": ["warn"],
      "no-return-assign": ["off"]
    }
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
